{
  "name": "picidae",
  "version": "1.1.10",
  "description": "Picidae is a document generator which has gentle experience. :dash:",
  "main": "lib/index.js",
  "scripts": {
    "link": "ln -sf ./ node_modules/picidae",
    "test": "mocha",
    "build": "babel src --out-dir lib -D",
    "prepublish": "npm run build",
    "gh-pages": "cd docs && NODE_DEBUG=gh-pages picidae gh-pages",
    "bd": "cd docs && picidae build -f",
    "page": "npm run bd && npm run gh-pages",
    "dev": "babel src --out-dir lib -Dw"
  },
  "bin": {
    "picidae": "./bin/picidae"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "transform-decorators-legacy",
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  },
  "files": [
    "lib",
    "bin",
    "theme",
    "exports"
  ],
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/picidaejs/picidae.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/picidaejs/picidae/issues"
  },
  "homepage": "https://github.com/picidaejs/picidae#readme",
  "dependencies": {
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.26.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chalk": "^2.1.0",
    "chokidar": "^1.7.0",
    "cli-spinner": "^0.2.6",
    "commander": "^2.11.0",
    "copy-dir": "^0.3.0",
    "css-loader": "^0.28.7",
    "del": "^3.0.0",
    "express": "^4.15.5",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^1.1.4",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "history": "^3.3.0",
    "html-to-react": "^1.3.1",
    "json-loader": "^0.5.7",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "loader-utils": "^1.1.0",
    "md5": "^2.2.1",
    "mkdirp": "^0.5.1",
    "moment": "^2.18.1",
    "node-notifier": "^5.1.2",
    "nprogress": "^0.2.0",
    "nunjucks": "^3.0.1",
    "picidae-plugin-utils": "^0.0.3",
    "postcss-loader": "^1.3.3",
    "prop-types": "^15.6.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-hot-loader": "^3.0.0-beta.3",
    "react-router": "^3.0.5",
    "remark": "^8.0.0",
    "remark-align": "^1.0.7",
    "remark-autolink-headings": "^5.0.0",
    "remark-gemoji-to-emoji": "^1.1.0",
    "remark-highlight.js": "^5.0.0",
    "remark-html": "^6.0.1",
    "remark-slug": "^4.2.3",
    "style-loader": "^0.19.0",
    "unist-util-visit": "^1.1.3",
    "url-loader": "^0.5.9",
    "webpack": "^1.15.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1",
    "yaml-front-matter": "^3.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "gh-pages": "^1.0.0",
    "mocha": "^3.5.3",
    "picidae-tools": "~0.0.3",
    "picidae-transformer-file-syntax": "^0.0.1",
    "picidae-transformer-react-render": "^1.0.9"
  },
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "test"
  }
}
