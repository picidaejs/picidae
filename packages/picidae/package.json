{
  "name": "picidae",
  "version": "3.0.0",
  "scripts": {
    "dist": "rimraf lib && tsc",
    "dev": "npx dist -- -w",
    "prepare": "npx dist",
    "changelog": "conventional-changelog -p angular -i ../../CHANGELOG.md -s -r 0 && git add ../../CHANGELOG.md"
  },
  "dependencies": {
    "debug": "^3.1.0",
    "remark": "^9.0.0"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.{j,t}s?(x)",
      "**/?(*.)(spec|test).{j,t}s?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
